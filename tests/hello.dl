import std.stdio;

int add(int a, int b) {
    return a + b;
}

void main() {
    int[] arr = [1, 2, 3];
    arr[0] = 42;
    write(arr[0]);
    writeln("Enter two numbers:");
    int x, y;
    readf(" %s %s", &x, &y);
    int result = add(x, y);
    writeln("The sum of ", x, " and ", y, " is ", result);

    writeln("Counting from 1 to ", result, ":");
    foreach(i; 1 .. result + 1) {
        write(i, " ");
    }
    writeln();
}
