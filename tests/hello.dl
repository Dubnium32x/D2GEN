int square(int x) {
    return x * x;
}

void main() {
    int[] nums = [1, 2, 3, 4, 5];
    int[] squares;
    
    // Map operation
    foreach (n; nums) {
        squares ~= square(n);
    }
    
    // Reduction
    int sum = 0;
    foreach (s; squares) {
        sum += s;
    }
    print("Sum of squares: ", sum);
}