struct Point {
    int x;
    int y;
};

void printPoint(Point p) {
    writeln(p.x);
    writeln(p.y);
}

void main() {
    // Struct and array of structs
    Point p;
    p.x = 10;
    p.y = 20;

    Point arr[2];
    arr[0].x = 1;
    arr[0].y = 2;
    arr[1].x = 3;
    arr[1].y = 4;

    // Function pointer
    void function(Point) fp;
    fp = &printPoint;
    fp(p);
    fp(arr[1]);

    // String and built-in methods
    string s;
    writeln(s.empty());
    // Uncomment if you support length:
    // writeln(s.length);

    // Control flow
    int sum = 0;
    for (int i = 0; i < 2; i = i + 1) {
        sum = sum + arr[i].x;
    }
    writeln(sum);

    // If/else
    if (sum > 3) {
        writeln(42);
    } else {
        writeln(0);
    }
}