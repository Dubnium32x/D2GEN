void main() {
    // Define a simple array
    int[] numbers = [1, 2, 3, 4, 5];

    // Print the array
    writeln("Array: ", numbers);

    // Access elements
    writeln("First element: ", numbers[0]);
    writeln("Last element: ", numbers[$ - 1]);

    // Modify an element
    numbers[2] = 10;
    writeln("Modified Array: ", numbers);

    // Iterate over the array
    writeln("Array elements:");
    foreach (num; numbers) {
        writeln(num);
    }
}